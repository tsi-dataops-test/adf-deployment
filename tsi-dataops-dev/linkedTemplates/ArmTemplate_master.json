{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tsi-dataops-dev"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"DataLake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DataLake'"
		},
		"bigdataKeyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://bigdata-keyvault.vault.azure.net/"
		},
		"NYCDatasets_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().url}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tsi-dataops-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"DataLake_connectionString": {
						"value": "[parameters('DataLake_connectionString')]"
					},
					"bigdataKeyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('bigdataKeyvault_properties_typeProperties_baseUrl')]"
					},
					"NYCDatasets_properties_typeProperties_url": {
						"value": "[parameters('NYCDatasets_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}